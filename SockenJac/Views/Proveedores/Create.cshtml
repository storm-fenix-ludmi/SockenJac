@model SockenJac.Models.Proveedores

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Proveedores</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NombreProveedores" class="control-label"></label>
                <input asp-for="NombreProveedores" class="form-control" />
                <span asp-validation-for="NombreProveedores" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<a asp-controller="Home" asp-action="Index" class="btn btn-secondary">Volver a Productos</a>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
